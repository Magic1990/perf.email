    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.7.0",
    "eslint-loader": "^1.3.0",
    "express": "^4.13.4",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.15.0",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "svg-sprite-loader": "0.0.18",
    "tachyons": "^4.0.0-beta.27",
    "uglify": "^0.1.5",
    "uglify-loader": "^1.3.0",
